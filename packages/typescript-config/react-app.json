{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React App",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "noEmit": true,
    "jsx": "react-jsx"
  }
}
